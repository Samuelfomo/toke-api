{
  "name": "toke-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ts-node src/server.ts",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "start": "node dist/server.js",
    "stop": "pkill -f \"node dist/server.js\"",
    "startAuto": "npm run build && pm2 start dist/server.js --name toke-api",
    "restartAuto": "npm run build && pm2 restart toke-api",
    "reloadAuto": "pm2 reload toke-api",
    "stopAuto": "pm2 stop toke-api",
    "logsAuto": "pm2 logs toke-api",
    "verifyStartApp": "pm2 list",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx --max-warnings 0",
    "redisTest": "npx ts-node scripts/manageUserAuth.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
